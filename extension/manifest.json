{
  "name": "SKK Japanese Input (dev)",
  "version": "0.1.13",
  "manifest_version": 2,
  "description": "SKK implementation for ChromeOS Input Method API",
  "background": {
    "scripts": [
      "skk.js",
      "roman_table.js",
      "dictionary_loader.js",
      "ascii_modes.js",
      "roman_modes.js",
      "preedit_modes.js",
      "conversion_modes.js",
      "pako_inflate.es5.min.js",
      "main.js"
    ]
  },
  "options_page": "options.html",
  "input_components": [
    {
      "name": "SKK (dev)(for US keyboard)",
      "type": "ime",
      "id": "net.hydrakecat.skk.us.dev",
      "description": "An SKK clone",
      "language": "ja",
      "layouts": ["us"]
    },{
      "name": "SKK (dev)(for Japanese keyboard)",
      "type": "ime",
      "id": "net.hydrakecat.skk.jp.dev",
      "description": "An SKK clone",
      "language": "ja",
      "layouts": ["jp"]
    },{
      "name": "SKK (dev)(for Dvorak keyboard)",
      "type": "ime",
      "id": "net.hydrakecat.skk.dvorak.dev",
      "description": "An SKK clone",
      "language": "ja",
      "layouts": ["us-dvorak"]
    }
  ],
  "permissions": [
    "input",
    "storage",
    "unlimitedStorage"
  ]
}
